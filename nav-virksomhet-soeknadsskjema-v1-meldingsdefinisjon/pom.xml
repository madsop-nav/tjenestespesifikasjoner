<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		 xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<parent>
		<groupId>no.nav.tjenestespesifikasjoner</groupId>
		<artifactId>tjenestespesifikasjoner</artifactId>
		<version>0-SNAPSHOT</version>
	</parent>

	<modelVersion>4.0.0</modelVersion>
	<artifactId>nav-virksomhet-soeknadsskjema-v1-meldingsdefinisjon</artifactId>
	<version>0-SNAPSHOT</version>
	<packaging>jar</packaging>

	<properties>
		<xsd.location>src/main/xsd</xsd.location>
	</properties>

	<build>
		<pluginManagement>
			<plugins>
				<plugin>
					<groupId>org.jvnet.jaxb2.maven2</groupId>
					<artifactId>maven-jaxb2-plugin</artifactId>
					<configuration>
						<schemaDirectory>${xsd.location}</schemaDirectory>
					</configuration>
				</plugin>
				<plugin>
					<groupId>org.apache.maven.plugins</groupId>
					<artifactId>maven-javadoc-plugin</artifactId>
					<executions>
						<execution>
							<id>attach-javadocs</id>
							<goals>
								<goal>jar</goal>
							</goals>
						</execution>
					</executions>
					<configuration>
						<doclint>none</doclint>
						<quiet>true</quiet>
					</configuration>
				</plugin>
			</plugins>
		</pluginManagement>
	</build>


	<scm>
		<developerConnection>scm:git:git@github.com:navikt/tjenestespesifikasjoner.git</developerConnection>
		<connection>scm:git:git@github.com:navikt/tjenestespesifikasjoner.git</connection>
		<url>https://github.com/navikt/tjenestespesifikasjoner</url>
		<tag>HEAD</tag>
	</scm>
</project>
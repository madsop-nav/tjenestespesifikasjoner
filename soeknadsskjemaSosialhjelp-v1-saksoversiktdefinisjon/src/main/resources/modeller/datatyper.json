{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "definitions": {
    "innsendtSoknad": {
      "title": "En innsendt søknad",
      "type": "object",
      "javaType": "no.nav.sbl.soknadsosialhjelp.tjeneste.saksoversikt.InnsendtSoknad",
      "properties": {
        "behandlingsId": {
          "title": "BehandlingsIden søknaden har i Sosialhjelp-systemet",
          "type": "string"
        },
        "temanavn": {
          "title": "Overskrift for gruppen av søknader",
          "type": "string"
        },
        "avsender": {
          "title": "Hvem som har sendt dokumentet",
          "$ref": "#/definitions/part"
        },
        "mottaker": {
          "title": "Hvem dokumentet er sendt til",
          "$ref": "#/definitions/part"
        },
        "lenke": {
          "title": "Lenke for å ta bruker til søknaden i sosialhjelp-systemet",
          "type": "string"
        },
        "innsendtDato": {
          "title": "Tidspunktet søknaden ble sendt",
          "type": "string",
          "format": "date-time"
        },
        "hoveddokument": {
          "title": "Informasjon om hoveddokumentet som bruker sendte",
          "type": "object",
          "properties": {
            "tittel": {
              "title": "Visningsnavn på hoveddokumentet",
              "type": "string"
            }
          }
        },
        "vedlegg": {
          "title": "Liste over vedlegg som ble lagt ved søknaden",
          "type": "array",
          "items": {
            "$ref": "#/definitions/vedlegg"
          }
        }
      }
    },
    "vedlegg": {
      "title": "Informasjon om et vedlegg",
      "type": "object",
      "properties": {
        "tittel": {
          "title": "Visningsnavn på vedlegget",
          "type": "string"
        }
      }
    },
    "part": {
      "title": "Informasjon om de i hver ende av en søknad",
      "type": "object",
      "javaType": "no.nav.sbl.soknadsosialhjelp.tjeneste.saksoversikt.Part",
      "properties": {
        "visningsNavn": {
          "title": "Navnet som skal vises for denne parten",
          "type": "string"
        },
        "type": {
          "title": "Hvilken type part det er",
          "enum": ["BRUKER", "NAV"]
        }
      }
    }

  }
}
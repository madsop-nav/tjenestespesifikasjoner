<jaxb:bindings xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
               xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
               jaxb:extensionBindingPrefixes="xjc" jaxb:version="2.0">

    <jaxb:globalBindings>
        <xjc:javaType name="java.time.OffsetDateTime" xmlType="xsd:dateTime" adapter="com.migesok.jaxb.adapter.javatime.OffsetDateTimeXmlAdapter"/>
        <xjc:javaType name="java.time.LocalDate" xmlType="xsd:date" adapter="com.migesok.jaxb.adapter.javatime.LocalDateXmlAdapter"/>
    </jaxb:globalBindings>

    <jaxb:bindings schemaLocation="no/nav/tjeneste/virksomhet/person/v2/informasjon/person/person.xsd" node="/xs:schema" >
        <jaxb:schemaBindings>
            <jaxb:nameXmlTransform>
                <jaxb:typeName prefix="WS"/>
                <jaxb:anonymousTypeName prefix="WS"/>
            </jaxb:nameXmlTransform>

        </jaxb:schemaBindings>
    </jaxb:bindings>

    <jaxb:bindings schemaLocation="no/nav/tjeneste/virksomhet/person/v2/feil/feil.xsd" node="/xs:schema">
        <jaxb:schemaBindings>
            <jaxb:nameXmlTransform>
                <jaxb:typeName prefix="WS"/>
                <jaxb:anonymousTypeName prefix="WS"/>
            </jaxb:nameXmlTransform>
        </jaxb:schemaBindings>
    </jaxb:bindings>

    <jaxb:bindings schemaLocation="no/nav/tjeneste/virksomhet/person/v2/meldinger/meldinger.xsd" node="/xs:schema">
        <jaxb:schemaBindings>
            <jaxb:nameXmlTransform>
                <jaxb:typeName prefix="WS"/>
                <jaxb:anonymousTypeName prefix="WS"/>
            </jaxb:nameXmlTransform>
        </jaxb:schemaBindings>
    </jaxb:bindings>

</jaxb:bindings>

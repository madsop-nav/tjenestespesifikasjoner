<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">



	<parent>
		<groupId>no.nav.tjenestespesifikasjoner</groupId>
		<artifactId>tjenestespesifikasjoner</artifactId>
		<version>0-SNAPSHOT</version>
	</parent>
	<modelVersion>4.0.0</modelVersion>
	<artifactId>melding-virksomhet-dokumentproduksjon</artifactId>
	<version>0-SNAPSHOT</version>
	<properties>
		<wsdl.directory>src/main/wsdl</wsdl.directory>
	</properties>

	<build>
		<plugins>
			<plugin>
				<groupId>org.jvnet.jaxb2.maven2</groupId>
				<artifactId>maven-jaxb2-plugin</artifactId>
				<executions>
					<execution>
						<id>generate-jaxb-dokmot</id>
						<goals>
							<goal>generate</goal>
						</goals>
						<configuration>
							<generatePackage>no.nav.meldinger.virksomhet.dokumentproduksjon</generatePackage>
							<schemaDirectory>src/main/resources/no/nav/meldinger/virksomhet/dokumentproduksjon/xsd
							</schemaDirectory>
							<bindingDirectory>src/main/resources/no/nav/meldinger/virksomhet/dokumentproduksjon/bindings
							</bindingDirectory>
							<generateDirectory>target/generated-sources</generateDirectory>
							<strict>false</strict>
							<schemaExcludes>*-skipbrevdata.xsd</schemaExcludes>
							<plugins>
								<plugin>
									<groupId>org.jvnet.jaxb2_commons</groupId>
									<artifactId>jaxb2-basics</artifactId>
									<version>1.11.1</version>
								</plugin>
								<plugin>
									<groupId>org.jvnet.jaxb2_commons</groupId>
									<artifactId>jaxb2-basics-annotate</artifactId>
									<version>1.0.4</version>
								</plugin>
								<plugin>
									<groupId>org.jvnet.jaxb2_commons</groupId>
									<artifactId>jaxb2-fluent-api</artifactId>
									<version>3.0</version>
								</plugin>
							</plugins>
							<args>
								<arg>-Xannotate</arg>
								<arg>-Xfluent-api</arg>
							</args>
						</configuration>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<artifactId>maven-source-plugin</artifactId>
				<executions>
					<execution>
						<id>include-sources</id>
						<goals>
							<goal>jar-no-fork</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>
	<scm>
		<developerConnection>scm:git:git@github.com:navikt/tjenestespesifikasjoner.git</developerConnection>
		<connection>scm:git:git@github.com:navikt/tjenestespesifikasjoner.git</connection>
		<url>https://github.com/navikt/tjenestespesifikasjoner</url>
		<tag>HEAD</tag>
	</scm>
	<name>${project.artifactId}</name>
</project>
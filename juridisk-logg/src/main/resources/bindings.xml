<jxb:bindings version="1.0"
  xmlns:jxb="http://java.sun.com/xml/ns/jaxb"
  xmlns:xs="http://www.w3.org/2001/XMLSchema">

    <jxb:bindings schemaLocation="archiver.xsd" node="/xs:schema">
        <jxb:schemaBindings>
            <jxb:nameXmlTransform>
                <jxb:typeName prefix="WS"/>
                <jxb:anonymousTypeName prefix="WS"/>
            </jxb:nameXmlTransform>
        </jxb:schemaBindings>
    </jxb:bindings>
    <jxb:bindings schemaLocation="ws-addr.xsd" node="/xs:schema">
        <jxb:bindings node="//xs:complexType[@name='AttributedURIType']">
            <jxb:class ref="org.w3._2005._08.addressing.AttributedURIType"/>
        </jxb:bindings>
    </jxb:bindings>
</jxb:bindings>

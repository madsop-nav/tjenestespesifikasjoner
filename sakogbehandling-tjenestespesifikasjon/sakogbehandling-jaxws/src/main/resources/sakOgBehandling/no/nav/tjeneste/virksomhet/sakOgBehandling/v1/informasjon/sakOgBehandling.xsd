	<xs:complexType name="Behandling" abstract="true">
		<xs:annotation>
			<xs:documentation>BEGREP-558
Alt som skal behandles med tilknytning til NAV er en saksbehandling. Det kan være bruker som fremmer et krav, bruker som simulerer en ytelse, veiledning, osv

Det må avklares når en sak skal opprettes og om alle behandlinger skal være knyttet til en sak.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="behandlingsId" type="informasjon:BehandlingID" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifiserer behandlingen.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="behandlingstype" type="informasjon:Behandlingstyper" minOccurs="1" maxOccurs="1"/>
			<xs:element name="applikasjon" type="informasjon:Applikasjoner" minOccurs="1" maxOccurs="1"/>
			<xs:element name="behandlingsstatus" type="informasjon:Behandlingsstatuser" minOccurs="1" maxOccurs="1"/>
			<xs:element name="primaerBehandling" type="informasjon:PrimaerBehandling" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifiserer den behandling som denne behandling er basert på. Forholdet kan enten være forrige eller eidAv eller basertPå.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="sekundaerBehandling" type="informasjon:SekundaerBehandling" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Identifiserer en eller flere behandlinger som denne behandling har et forhold til. Forholdet kan enten være seOgså eller skalVarlses.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="start" type="xs:dateTime" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Tidspunktet behandlingen ble påbegynt.</xs:documentation>
				</xs:annotation>
			</xs:element>						
			<xs:element name="sisteoppdatering" type="xs:dateTime" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Tidspunktet det sist skjedde en endring i status av denne behandling.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="slutt" type="xs:dateTime" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Tidspunktet behandlingen ble avsluttet.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Behandlingstid">
		<xs:annotation>
			<xs:documentation>Entitet som angir tid i form av verdi og type tid (f. eks uker, dager, timer, etc.)</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="tid" type="xs:integer" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Angitt tid</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="type" type="informasjon:Behandlingstidtyper" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Angir tidsenhet (f. eks. uker, dager, timer, etc.).</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BehandlingVS">
		<xs:annotation>
			<xs:documentation>Behandlingsentitet som dekker spesifikke behov sett fra Virksomhetsstyring</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="informasjon:Behandling">
				<xs:sequence>
					<xs:element name="sisteBehandlingssteg" type="informasjon:Behandlingsstegtyper" minOccurs="0" maxOccurs="1"/>
					<xs:element name="applikasjonSakREF" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Identifiserer i hvilken sak behandlingen inngår, i gjeldende applikasjon (fagsystem) dersom en slik eksisterer.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="applikasjonBehandlingREF" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Refererer til en eventuell mer spesifikk entitet i applikasjonen som svarer til behandlingen i MOD.</xs:documentation>
						</xs:annotation>
					</xs:element>
                    <xs:element name="ansvarligEnhetREF" type="informasjon:OrganisasjonsenhetID" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Ansvarlig enhet som har utført/utfører behandlingen.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="avslutningsstatus" type="informasjon:Avslutningsstatuser" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Den status behandlingen har idet den avsluttes</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="normertBehandlingstid" type="informasjon:Behandlingstid" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Den tid det er forventet at en behandling av den gitte type skal ta.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="frist" type="xs:dateTime" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Det tidspunkt NAV skal være ferdig med sin behandling.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="sisteBehandlingsstegREF" type="informasjon:BehandlingsstegID" minOccurs="1" maxOccurs="1"/>
					<xs:element name="sisteBehandlingsstegtype" type="informasjon:Behandlingsstegtyper" minOccurs="1" maxOccurs="1"/>
					<xs:element name="sisteVenteaarsak" type="informasjon:Venteaarsaker" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="Kodeverdi" abstract="true">
		<xs:simpleContent>
			<xs:extension base="xs:string">
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
</xs:schema>

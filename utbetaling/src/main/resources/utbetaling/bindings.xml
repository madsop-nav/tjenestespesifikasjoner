<jaxb:bindings version="1.0"
              xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
              xmlns:xs="http://www.w3.org/2001/XMLSchema"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/jaxb http://java.sun.com/xml/ns/jaxb/bindingschema_2_0.xsd">

    <jaxb:bindings schemaLocation="no/nav/virksomhet/okonomi/utbetaling/utbetaling.xsd" node="/xs:schema">
        <jaxb:schemaBindings>
            <jaxb:nameXmlTransform>
                <jaxb:typeName prefix="WS"/>
                <jaxb:anonymousTypeName prefix="WS"/>
            </jaxb:nameXmlTransform>
        </jaxb:schemaBindings>
    </jaxb:bindings>

    <jaxb:bindings schemaLocation="no/nav/virksomhet/tjenester/utbetaling/feil/feil.xsd" node="/xs:schema">
        <jaxb:schemaBindings>
            <jaxb:nameXmlTransform>
                <jaxb:typeName prefix="WS"/>
                <jaxb:anonymousTypeName prefix="WS"/>
            </jaxb:nameXmlTransform>
        </jaxb:schemaBindings>
    </jaxb:bindings>

    <jaxb:bindings schemaLocation="no/nav/virksomhet/tjenester/utbetaling/meldinger/meldinger.xsd" node="/xs:schema">
        <jaxb:schemaBindings>
            <jaxb:nameXmlTransform>
                <jaxb:typeName prefix="WS"/>
                <jaxb:anonymousTypeName prefix="WS"/>
            </jaxb:nameXmlTransform>
        </jaxb:schemaBindings>
    </jaxb:bindings>

    <jaxb:globalBindings>
        <jaxb:javaType
                name="org.joda.time.DateTime"
                xmlType="xs:date"
                parseMethod="no.nav.sbl.dialogarena.adapters.simple.DateTimeAdapter.unmarshal"
                printMethod="no.nav.sbl.dialogarena.adapters.simple.DateTimeAdapter.marshal"
                />
        <jaxb:serializable />
    </jaxb:globalBindings>

</jaxb:bindings>

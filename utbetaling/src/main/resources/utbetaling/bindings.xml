<jxb:bindings version="1.0"
              xmlns:jxb="http://java.sun.com/xml/ns/jaxb"
              xmlns:xs="http://www.w3.org/2001/XMLSchema"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/jaxb http://java.sun.com/xml/ns/jaxb/bindingschema_2_0.xsd">

    <jxb:bindings schemaLocation="no/nav/virksomhet/okonomi/utbetaling/utbetaling.xsd" node="/xs:schema">
        <jxb:schemaBindings>
            <jxb:nameXmlTransform>
                <jxb:typeName prefix="WS"/>
                <jxb:anonymousTypeName prefix="WS"/>
            </jxb:nameXmlTransform>
        </jxb:schemaBindings>
    </jxb:bindings>

    <jxb:bindings schemaLocation="no/nav/virksomhet/tjenester/utbetaling/feil/feil.xsd" node="/xs:schema">
        <jxb:schemaBindings>
            <jxb:nameXmlTransform>
                <jxb:typeName prefix="WS"/>
                <jxb:anonymousTypeName prefix="WS"/>
            </jxb:nameXmlTransform>
        </jxb:schemaBindings>
    </jxb:bindings>

    <jxb:bindings schemaLocation="no/nav/virksomhet/tjenester/utbetaling/meldinger/meldinger.xsd" node="/xs:schema">
        <jxb:schemaBindings>
            <jxb:nameXmlTransform>
                <jxb:typeName prefix="WS"/>
                <jxb:anonymousTypeName prefix="WS"/>
            </jxb:nameXmlTransform>
        </jxb:schemaBindings>
    </jxb:bindings>

    <jxb:globalBindings>
        <jxb:javaType
                name="org.joda.time.DateTime"
                xmlType="xs:dateTime"
                parseMethod="no.nav.sbl.dialogarena.utbetaling.adapters.DateTimeAdapter.unmarshal"
                printMethod="no.nav.sbl.dialogarena.utbetaling.adapters.DateTimeAdapter.marshal"
                />
    </jxb:globalBindings>

</jxb:bindings>

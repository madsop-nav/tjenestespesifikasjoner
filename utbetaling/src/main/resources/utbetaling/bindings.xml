<jaxb:bindings version="1.0"
               xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
               xmlns:xs="http://www.w3.org/2001/XMLSchema"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
               xsi:schemaLocation="http://java.sun.com/xml/ns/jaxb http://java.sun.com/xml/ns/jaxb/bindingschema_2_0.xsd">

    <jaxb:bindings schemaLocation="no/nav/tjeneste/virksomhet/utbetaling/v1/informasjon/informasjon.xsd" node="/xs:schema" >
        <jaxb:schemaBindings>
            <jaxb:nameXmlTransform>
                <jaxb:typeName prefix="WS"/>
                <jaxb:anonymousTypeName prefix="WS"/>
            </jaxb:nameXmlTransform>

        </jaxb:schemaBindings>
    </jaxb:bindings>

    <jaxb:bindings schemaLocation="no/nav/tjeneste/virksomhet/utbetaling/v1/feil/feil.xsd" node="/xs:schema">
        <jaxb:schemaBindings>
            <jaxb:nameXmlTransform>
                <jaxb:typeName prefix="WS"/>
                <jaxb:anonymousTypeName prefix="WS"/>
            </jaxb:nameXmlTransform>
        </jaxb:schemaBindings>
    </jaxb:bindings>

    <jaxb:bindings schemaLocation="no/nav/tjeneste/virksomhet/utbetaling/v1/meldinger/meldinger.xsd" node="/xs:schema">
        <jaxb:schemaBindings>
            <jaxb:nameXmlTransform>
                <jaxb:typeName prefix="WS"/>
                <jaxb:anonymousTypeName prefix="WS"/>
            </jaxb:nameXmlTransform>
        </jaxb:schemaBindings>
    </jaxb:bindings>

    <jaxb:globalBindings>
        <xjc:javaType
                name="org.joda.time.DateTime"
                xmlType="xs:date"
                adapter="no.nav.sbl.dialogarena.adapters.utbetaling.UtbetalingDateAdapter"
                />
        <xjc:serializable />
    </jaxb:globalBindings>

</jaxb:bindings>

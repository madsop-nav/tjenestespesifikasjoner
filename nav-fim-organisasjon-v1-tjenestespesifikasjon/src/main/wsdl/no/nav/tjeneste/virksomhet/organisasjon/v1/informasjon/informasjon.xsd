					<xs:element name="inngaarIJuridiskEnhet" type="informasjon:InngaarIJuridiskEnhet" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="orgleddDetaljer" type="informasjon:OrgLeddDetaljer" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="OrgLeddDetaljer">
		<xs:annotation>
			<xs:documentation>Informasjon om organisasjonsledd 
enhetstype=ORGL</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="enhetstype" type="xs:string" minOccurs="1" maxOccurs="1">

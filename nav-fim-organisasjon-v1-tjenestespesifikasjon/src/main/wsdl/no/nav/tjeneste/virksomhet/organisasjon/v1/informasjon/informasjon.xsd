			<xs:element name="juridiskEnhet" type="informasjon:JuridiskEnhet" minOccurs="1" maxOccurs="1"/>
		</xs:sequence>
		<xs:attributeGroup ref="metadata:Bruksperiode"/>
		<xs:attributeGroup ref="metadata:Gyldighetsperiode"/>
	</xs:complexType>
